{
  "branches": ["main"],
  "plugins": [
    "@semantic-release/commit-analyzer", 
    "@semantic-release/release-notes-generator",
    ["@semantic-release/exec", {
      "prepareCmd": "git tag -d v${nextRelease.version} && git tag -a v${nextRelease.version} -m \"Release v${nextRelease.version}\" && git push --tags",
      "publishCmd": "echo \"{\"name\": \"v${nextRelease.version}\", \"url\": \"https://github.com/Kozoaku/XIV_Databar/releases/tag/v${nextRelease.version}\"}",
      "successCmd": "Pushed next release tag"
    }]
  ]
}